
    <% @listings.each_slice(4).each do |group| %>
    <div class='row'>
      <% group.each do |listing| %>

            <div class="col-sm-3">
              <div class="card">
                <div class="card-body">
                <%=link_to listing_path(listing) do%>
                  <img  class="w-100 shadow-1-strong rounded"
                   src= <%= listing.primary_image_url %>
                   alt="Boat on Calm Water"/>
                  <h5 class="card-title"><%= listing.city %></h5>
                  <h5 class="card-title"><%=listing.country%></h5>
                  <h6 class="card-title"><%=listing.price%> $</h6>
                 <% end %>
                </div>
              </div>
            </div>


      <% end %>
      </div>
    <% end %>
